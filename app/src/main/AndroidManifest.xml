<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.mtechyard.newpizzayum">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.NewPizzaYum"
        tools:targetApi="m">
        <activity android:name=".OrderReview"></activity>
        <activity android:name=".offers_and_discounts" />
        <activity android:name=".order_detail" />
        <activity
            android:name=".user_cart"
            android:label="@string/title_activity_user_cart"
            android:theme="@style/Theme.NewPizzaYum.NoActionBar" />
        <activity android:name=".user_address" />
        <activity android:name=".user_order" />
        <activity android:name=".user_profile" />
        <activity
            android:name=".user_details_form"
            android:label="@string/title_activity_user_details_form"
            android:theme="@style/Theme.NewPizzaYum.NoActionBar" />
        <activity android:name=".otp" />
        <activity android:name=".user_auth" />
        <activity android:name=".home" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.razorpay"
            android:value="rzp_test_RuC9zsLH3fQ27s"></meta-data>
    </application>

</manifest>